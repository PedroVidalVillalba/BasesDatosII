<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.admin.hostalaria.AdminHostalariaController"
            prefHeight="400.0" prefWidth="600.0">

    <TableView fx:id="restaurantTable" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="nomeColumn" text="Nombre" />
            <TableColumn fx:id="aforoColumn" text="Aforo" />
            <TableColumn fx:id="horainicioColumn" text="Hora inicio" />
            <TableColumn fx:id="horafinColumn" text="Hora fin" />
            <TableColumn fx:id="zonaColumn" text="Zona" />
        </columns>
    </TableView>


</AnchorPane>
